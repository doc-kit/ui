{
  "name": "@shockits/doc-ui",
  "version": "1.0.0",
  "description": "A collection of React UI components to enhance documentation",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/shockits/doc-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/shockits/doc-ui.git"
  },
  "bugs": {
    "url": "https://github.com/shockits/doc-ui/issues"
  },
  "author": {
    "name": "Michael Donahue",
    "email": "mtdonahue1@gmail.com",
    "url": "https://github.com/shockits/doc-ui"
  },
  "keywords": [
    "doc-ui",
    "docs",
    "documentation",
    "react-components",
    "react",
    "shockits",
    "typescript",
    "ui"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:babel": "babel src -d lib --extensions '.ts,.tsx' --source-maps inline",
    "build": "yarn build:types && yarn build:babel",
    "ts": "tsc --noEmit",
    "ts:watch": "yarn ts --watch",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "commit": "commit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-angular": "7.1.2",
    "@commitlint/prompt-cli": "7.2.1",
    "@commitlint/travis-cli": "7.2.1",
    "@semantic-release/changelog": "3.0.1",
    "@semantic-release/git": "7.0.5",
    "@storybook/addon-actions": "4.0.4",
    "@storybook/addon-links": "4.0.4",
    "@storybook/addons": "4.0.4",
    "@storybook/cli": "4.0.4",
    "@storybook/react": "4.0.4",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.9",
    "@types/react": "16.4.18",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__react": "4.0.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.4",
    "babel-plugin-emotion": "9.2.11",
    "classnames": "2.2.6",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "husky": "1.1.3",
    "jest": "23.6.0",
    "lint-staged": "8.0.4",
    "prettier": "1.14.3",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-test-renderer": "16.6.0",
    "semantic-release": "15.10.8",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
    "webpack": "4.25.1"
  },
  "dependencies": {
    "emotion": "9.2.12"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "yarn prettier:fix",
      "git add"
    ]
  }
}
