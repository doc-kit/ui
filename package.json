{
  "name": "@shockits/doc-ui",
  "version": "1.2.0",
  "description": "A collection of React UI components to enhance your documentation.",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/shockits/doc-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/shockits/doc-ui.git"
  },
  "bugs": {
    "url": "https://github.com/shockits/doc-ui/issues"
  },
  "author": {
    "name": "Michael Donahue",
    "email": "mtdonahue1@gmail.com",
    "url": "https://github.com/shockits/doc-ui"
  },
  "keywords": [
    "doc-ui",
    "docs",
    "documentation",
    "react-components",
    "react",
    "shockits",
    "typescript",
    "ui"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build:babel": "babel src -d lib --extensions '.ts,.tsx'",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "yarn build:babel && yarn build:types",
    "ts": "tsc --noEmit",
    "ts:watch": "yarn ts --watch",
    "lint": "tslint --project . && stylelint 'src/**/*.tsx'",
    "lint:fix": "tslint --fix --project .",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "commit": "commit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook/build",
    "storybook:deploy": "gh-pages -d .storybook/build",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@commitlint/prompt-cli": "7.5.0",
    "@commitlint/travis-cli": "7.5.2",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@storybook/addon-actions": "4.1.6",
    "@storybook/addon-knobs": "4.1.6",
    "@storybook/addon-options": "4.1.6",
    "@storybook/addon-viewport": "4.1.6",
    "@storybook/react": "4.1.6",
    "@types/deepmerge": "2.2.0",
    "@types/enzyme": "3.1.17",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/hoist-non-react-statics": "3.0.1",
    "@types/jest": "23.3.14",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.0.11",
    "@types/react-test-renderer": "16.0.3",
    "@types/storybook-readme": "4.0.0",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-knobs": "4.0.0",
    "@types/storybook__addon-options": "3.2.3",
    "@types/storybook__react": "4.0.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.5",
    "babel-plugin-emotion": "10.0.6",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-test-renderer": "16.7.0",
    "semantic-release": "15.13.3",
    "storybook-readme": "4.0.5",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.5.2",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2",
    "webpack": "4.29.3"
  },
  "dependencies": {
    "@emotion/core": "10.0.6",
    "@emotion/styled": "10.0.6",
    "deepmerge": "3.1.0",
    "emotion-theming": "10.0.6",
    "facepaint": "1.2.1",
    "hoist-non-react-statics": "3.3.0",
    "polished": "2.3.3",
    "react-icons": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "concurrent": true,
    "linters": {
      "*.{ts,tsx}": [
        "yarn lint --fix",
        "yarn prettier:fix",
        "git add"
      ]
    }
  }
}
